<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scu.mapper.TemplateDataMapper">
    <select id="getAuditedTemplateData" resultType="HashMap">
        SELECT * FROM template_data_${templateId} WHERE status = 1
    </select>
    <select id="getUnAuditedTemplateData" resultType="HashMap">
        SELECT * FROM template_data_${templateId} WHERE status = 0
    </select>
</mapper>